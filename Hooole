        echo -e "${GREEN}â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•${NC}"
        
        echo -e "\n${YELLOW}ğŸ“Š Metode berhasil:${NC}"
        for method in "${methods[@]}"; do
            echo -e "${GREEN}  âœ… $method${NC}"
        done
        
        # Save success
        echo "$target | HACKED BY LYNNN | $ip | $(date) | Methods: ${methods[*]}" >> lynn_success.txt
        
        # Open browser
        xdg-open "$target" 2>/dev/null || echo -e "\n${CYAN}Buka: $target${NC}"
        
